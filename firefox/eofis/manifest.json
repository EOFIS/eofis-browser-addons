{
    "manifest_version": 2,
    "name": "EOFIS",
    "version": "0.1",
    "description": "Adds an entry to save information in EOFIS in the right-click menu.",
    "icons": {
        "16": "icons/logo.svg",
        "32": "icons/logo.svg",
        "48": "icons/logo.svg",
        "96": "icons/logo.svg"
    },
    "permissions" : [
        "contextMenus",
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/logo-32.png",
        "default_title": "Send to EOFIS",
        "default_popup": "popup/modify_note.html"
    },
    "background": {
        "scripts": [
            "eofis.js"
        ]
    }
}
